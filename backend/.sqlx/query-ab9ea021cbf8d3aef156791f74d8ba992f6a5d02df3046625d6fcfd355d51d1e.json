  "query": "\n            UPDATE jobs\n            SET\n                name = COALESCE($1, name),\n                description = COALESCE($2, description),\n                business_model = COALESCE($3, business_model),\n                updated_at = NOW()\n            WHERE id = $4 AND project_id = $5\n            RETURNING\n                id,\n                project_id,\n                name,\n                description,\n                business_model as \"business_model: BusinessModel\",\n                created_at,\n                updated_at\n            ",
